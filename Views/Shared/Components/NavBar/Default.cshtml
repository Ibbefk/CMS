@model IEnumerable<NavBarViewModel>

<header>
    <div class="top-bar">
        <div class="container">
            <span class="address">Klarabergsviadukten 90, 111 64 Stockholm</span>
        </div>
    </div>

    <div class="logo-nav">
        <div class="logo">
            <img src="/image/Brand.png" alt="Onatrix Logo" />
        </div>
        <nav>
            <ul class="nav-links">
                @foreach (var item in Model)
                {
                    <li><a href="@item.Url">@item.Title</a></li>
                }
            </ul>
            <div class="search-icon">
                <a href="#"><i class="icon-search"></i></a>
            </div>
        </nav>
    </div>
</header>
